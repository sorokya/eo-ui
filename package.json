{
  "name": "eo-ui",
  "version": "0.1.0",
  "description": "Component library based on Endless Online for react",
  "module": "index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "node ./build-atlas.js && tsc && rollup -c && cp package.json dist/ && cp README.md dist/ && cp LICENSE.txt dist/",
    "line": "biome check .",
    "format": "biome check --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o storybook-static",
    "deploy-storybook": "gh-pages -d storybook-static",
    "atlas": "node ./build-atlas.js"
  },
  "files": [
    "assets",
    "types",
    "index.js",
    "index.js.map",
    "styles.css"
  ],
  "keywords": [
    "react",
    "component",
    "library",
    "endless online",
    "eo"
  ],
  "author": "Richard Leek <richard@richardleek.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.20.0+sha256.47a3352808501b8d1ef20112273b6a5dcfa53d28a55bcce36d268e878bd6bfe9",
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@rollup/plugin-url": "^8.0.2",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-onboarding": "^10.0.8",
    "@storybook/react-vite": "^10.0.8",
    "@types/postcss-url": "^10.0.4",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/rollup-plugin-peer-deps-external": "^2.2.6",
    "@vueless/storybook-dark-mode": "^10.0.3",
    "fs-extra": "^11.3.2",
    "gh-pages": "^6.3.0",
    "globby": "^16.0.0",
    "lefthook": "2.0.4",
    "postcss-url": "^10.1.3",
    "rollup": "^4.53.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "spritesmith": "^3.5.1",
    "storybook": "^10.0.8",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
